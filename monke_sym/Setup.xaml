<Window x:Class="monke_sym.Setup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:monke_sym"
        mc:Ignorable="d"
        Title="Setup" Height="450" Width="800">
    <StackPanel Margin="10">
        <TextBlock FontWeight="Bold" Name="ChooseGender">Choose your monkeys gender</TextBlock>
        <RadioButton>Male</RadioButton>
        <RadioButton>Female</RadioButton>
        <RadioButton IsChecked="True">I tried asking and things got violent</RadioButton>
    </StackPanel>
</Window>
